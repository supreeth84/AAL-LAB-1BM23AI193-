#include <stdio.h>
#include <string.h>
int main() {
    char s[] = "nobody noticed him";
    char ms[] = "him";
    int n = strlen(s);
    int m = strlen(ms);
    for (int i = 0; i <= n - m; i++) {
        int j = 0;
        while (j < m && ms[j] == s[i + j]) {
            j++; 
        }
        if (j == m) {
            printf("Pattern found at index %d\n", i);
        }
    }
    return 0;
}
